# Copy this file to .env and fill in your values

# Your GCP project ID
PROJECT_ID=

# Region for Vertex AI (e.g., us-central1, europe-west1)
LOCATION=

# Gemini model to use (e.g., gemini-1.5-pro-002 or gemini-1.5-flash-002)
MODEL=gemini-1.5-pro-002

# Choose ONE retrieval backend:

# Option A) Vertex RAG Store (RAG Engine)
# Full corpus resource name:
# projects/your-project/locations/us-central1/ragCorpora/your-corpus-id
RAG_CORPUS=

# Option B) Vertex AI Search datastore (ID only, not full path)
DATA_STORE_ID=

# Retrieval tuning
SIMILARITY_TOP_K=5

# Server port
PORT=8080
